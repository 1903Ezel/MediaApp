{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "src": "/manifest.webmanifest", "dest": "/dist/manifest.webmanifest" },
    { "src": "/OneSignalSDKWorker.js", "dest": "/dist/OneSignalSDKWorker.js" },
    { "src": "/OneSignalSDKUpdaterWorker.js", "dest": "/dist/OneSignalSDKUpdaterWorker.js" },
    { "src": "/sw.js", "dest": "/dist/sw.js" },
    { "src": "/(.*)", "dest": "/dist/$1" }
  ]
}
